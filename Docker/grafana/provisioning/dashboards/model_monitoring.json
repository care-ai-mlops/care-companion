    {
      "type": "gauge",
      "title": "Current MMD Score (Sliding Window)",
      "gridPos": { "x": 0, "y": 1, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "drift_score{metric_type=\"mmd\"}",
          "legendFormat": "MMD Score",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "red", "value": 0.7 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "MMD Score History (Sliding Window)",
      "gridPos": { "x": 8, "y": 1, "w": 16, "h": 8 },
      "targets": [
        {
          "expr": "drift_score{metric_type=\"mmd\"}",
          "legendFormat": "MMD Score",
          "refId": "A"
        },
        {
          "expr": "drift_threshold{metric_type=\"mmd\"}",
          "legendFormat": "Threshold",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "unit": "percentunit"
        }
